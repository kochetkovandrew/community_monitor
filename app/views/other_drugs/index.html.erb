<p id="notice"><%= notice %></p>

<h1>Listing Other Drugs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_drugs.each do |other_drug| %>
      <tr>
        <td><%= other_drug.name %></td>
        <td><%= link_to 'Show', other_drug %></td>
        <td><%= link_to 'Edit', edit_other_drug_path(other_drug) %></td>
        <td><%= link_to 'Destroy', other_drug, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other drug', new_other_drug_path %>
